pipeline {
    agent any

    stages {
        stage('git checkout') {
            steps {
                git 'https://github.com/KavithaDonda/Java-Project.git'
            }
        }
        stage('build') {
            steps {
                sh "mvn clean package"
            }
        }
         stage('Docker') {
            steps {
                sh "sudo docker pull tomcat:9.0"
            }
        }
    }
}
